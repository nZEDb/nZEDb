{
  "authors": [
    {
      "name": "nZEDb Team"
    }
  ],
  "autoload": {
    "psr-4": {
      "nzedb\\": "nzedb/"
    },
    "classmap": [
      "www/pages/",
      "libs/PEAR/Net_NNTP/NNTP/",
      "libs/"
    ]
  },
  "config": {
    "sort-packages": false,
    "optimize-autoloader": true,
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "www/themes/shared/libs",
        "bower-asset-library": "www/themes/shared/libs"
      }
    },
    "process-timeout": 3600,
    "preferred-install": {
      "*": "source"
    },
    "vendor-dir": "libraries"
  },
  "description": "A Usenet Indexer",
  "extra": {
    "branch-alias": {
      "dev-dev": "0.x-dev"
    }
  },
  "homepage": "http://nzedb.com",
  "keywords": [
    "nZEDb",
    "usenet",
    "indexer",
    "lithium",
    "li3"
  ],
  "license": "GPL-3.0",
  "minimum-stability": "dev",
  "name": "nzedb/nzedb",
  "non-feature-branches": [
    "Latest-*"
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/niel/li3_docs"
    },
    {
      "type": "vcs",
      "url": "https://github.com/niel/li3_fixtures"
    },
    {
      "type": "vcs",
      "url": "https://github.com/niel/li3_quality"
    },
    {
      "type": "vcs",
      "url": "https://github.com/niel/lithium"
    },
    {
      "type": "vcs",
      "url": "https://github.com/nZEDb/Git.php"
    },
    {
      "type": "vcs",
      "url": "https://github.com/unionofrad/manual"
    },
    {
      "type": "vcs",
      "url": "https://github.com/zeebinz/rarinfo"
    }
  ],
  "require": {
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-exif": "*",
    "ext-filter": "*",
    "ext-gd": "*",
    "ext-hash": "*",
    "ext-iconv": "*",
    "ext-imagick": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-mysqlnd": "*",
    "ext-openssl": "*",
    "ext-pcre": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "ext-session": "*",
    "ext-simplexml": "*",
    "ext-sockets": "*",
    "ext-spl": "*",
    "ext-xmlwriter": "*",
    "ext-zlib": "*",
    "b3rs3rk/steamfront": "dev-master",
    "barracudanetworks/forkdaemon-php": "^1.0.7",
    "dborsatto/php-giantbomb": "dev-master",
    "google/recaptcha": "^1.1.2",
    "guzzlehttp/guzzle": "^6.3",
    "james-heinrich/getid3": "^1.9.12",
    "joshpinkney/tv-maze-php-api": "dev-master",
    "mhor/php-mediainfo": "^5.0@dev",
    "moinax/tvdb": "^1.0.4",
    "niel/li3_fixtures": "1.2.x-dev",
    "niel/lithium": "dev-php7.2-updates",
    "nzedb/Git.php": "dev-master",
    "php": "~7.0",
    "php-tmdb/api": "^2.1",
    "phpmailer/phpmailer": "^6.0.2",
    "smarty/smarty": "~3.1.29",
    "zeebinz/rarinfo": "~2.5.0"
  },
  "require-dev": {
    "unionofrad/manual": "1.1.x-dev"
  },
  "scripts": {
    "post-install-cmd": [
      "nzedb\\build\\Composer::postInstallCmd"
    ]
  },
  "suggest": {
    "ext-php-yenc": "Improves yEnc decoding speed for PHP 7.0+ (and 5.6)",
    "ext-xdebug": "For developement work"
  },
  "type": "project"
}
